<body style="background-color: #666666;">
	
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">
				<div class="login100-form validate-form">
					<%= form_for(resource, as: resource_name,class:"login100-form", url: session_path(resource_name), html: { data: { turbo: false} } ) do |f| %>
					<span class="login100-form-title p-b-43">
						Login to continue as admin
					</span>
					
					
					<div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
							<%= f.email_field :email, autofocus: true, autocomplete: "email",class:"input100" %>
						<span class="focus-input100"></span>
						 <%= f.label :email,class:"label-input100" %><br />
						
    				
					</div>
					
					
					<div class="wrap-input100 validate-input" data-validate="Password is required">
						<%= f.password_field :password, autocomplete: "current-password",class:"input100"  %>
						<span class="focus-input100"></span>
						 <%= f.label :password,class:"label-input100" %><br />
    				
					</div>

					<div class="flex-sb-m w-full p-t-3 p-b-32">
						<div class="contact100-form-checkbox">
							<% if devise_mapping.rememberable? %>
								<div class="field">
								<%= f.check_box :remember_me,  class:"input-checkbox100" ,id:"ckb1"%>
								<%= f.label :remember_me ,class:"label-checkbox100", for:"ckb1"%>
								</div>
							<% end %>
						</div>

					
					</div>
			

					<div class="container-login100-form-btn">
						<button class="login100-form-btn">
							Login
						</button>
					</div>
					
					<div class="text-center p-t-46 p-b-20">
						<span class="txt2">
						<%= render "admins/shared/error_messages", resource: resource %>
						<%= render "admins/shared/links" %>
						</span>
					</div>

					<div class="login100-form-social flex-c-m">
						<a href="#" class="login100-form-social-item flex-c-m bg1 m-r-5">
							<i class="fa fa-facebook-f" aria-hidden="true"></i>
						</a>

						<a href="#" class="login100-form-social-item flex-c-m bg2 m-r-5">
							<i class="fa fa-twitter" aria-hidden="true"></i>
						</a>
					</div>
					<% end %>
				</div>

				<div class="login100-more" style="background-image: url('images/bg-01.jpg');">
				</div>
			</div>
		</div>
	</div>
	
